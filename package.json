{
  "name": "jup-perps-client",
  "version": "1.0.0",
  "description": "TypeScript client for Jupiter Perpetuals Protocol generated from IDL using Codama",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "generate": "bun generate-client.ts && bun add-jsdoc.ts",
    "build": "bun generate && bun build src/index.ts --outdir dist --target node --format esm --sourcemap && tsc --project tsconfig.build.json",
    "dev": "bun --hot generate-client.ts",
    "example": "bun example.ts",
    "prepublishOnly": "bun run build",
    "publish-npm": "npm publish",
    "publish-dry": "npm publish --dry-run"
  },
  "keywords": [
    "solana",
    "jupiter",
    "perpetuals",
    "typescript",
    "defi",
    "trading"
  ],
  "author": "monakki",
  "license": "MIT",
  "homepage": "https://github.com/monakki/jup-perps-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/monakki/jup-perps-client.git"
  },
  "bugs": {
    "url": "https://github.com/monakki/jup-perps-client/issues"
  },
  "dependencies": {
    "@solana/kit": "^2.3.0"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.2.1",
    "@codama/renderers-js": "^1.2.14",
    "@codama/visitors": "^1.3.0",
    "@types/bun": "latest",
    "codama": "^1.3.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "funding": {
    "type": "custom",
    "url": "https://github.com/monakki/jup-perps-client#-support"
  }
}
